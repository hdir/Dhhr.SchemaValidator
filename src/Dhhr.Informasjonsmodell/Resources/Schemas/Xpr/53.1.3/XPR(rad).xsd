      <attribute name = "innmateHast" type = "npr:innmate_hastegrad"  use = "required" />
      <attribute name = "alderIDager" type = "integer"  use = "prohibited" />
      <attribute name = "omsorgsniva" type = "npr:omsorgsniva"  use = "required" />
