      <attribute name = "komNrHjem" type = "nprx:kommNr"  use = "required" />
      <attribute name = "bydel" type = "nprx:bydelNr"  use = "optional" />
      <attribute name = "inntilstand" type = "npr:inntilstand"  use = "optional" />
