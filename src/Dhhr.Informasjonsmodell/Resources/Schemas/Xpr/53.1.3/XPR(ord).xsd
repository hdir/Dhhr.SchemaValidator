      <attribute name = "arenafleksibel" type = "npr:ja_nei"  use = "optional" />
      <attribute name = "utTilstand" type = "npr:uttilstand"  use = "optional" />
      <attribute name = "sluttKode" type = "npr:avslutningsmate_rus"  use = "optional" />
